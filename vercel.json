{
  "version": 2,
  "builds": [
    {
      "src": "api/iyzico/create-payment.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/iyzico/callback.js", 
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/iyzico/create-payment",
      "dest": "/api/iyzico/create-payment.js"
    },
    {
      "src": "/api/iyzico/callback",
      "dest": "/api/iyzico/callback.js"
    },
    {
      "src": "/health",
      "dest": "/api/health.js"
    }
  ]
}